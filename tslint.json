{
  "defaultSeverity": "error",
  "extends": [
    "tslint:recommended",
    "tslint-config-prettier"
  ],
  "jsRules": {},
  "rules": {
    "ordered-imports": true,
    "interface-name": [true, "never-prefix"],
    "prefer-const": true,
    "no-implicit-dependencies": [true, "dev", [
        "config",
        "helpers",
        "controller",
        "types",
        "service"
      ]
    ]
  },
  "rulesDirectory": []
}
